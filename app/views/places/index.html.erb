<h1 class="container-fluid px-6">Places</h1>

<div class="container-fluid px-6">
  <p>
    <a class="btn btn-primary btn-sm" href="/places/new" role="button">New Place</a> <!--turns into button -->
  </p>

  <ul>
    <% for place in @places %>
      <li><a href="/places/<%= place.id %>"><%= place.name %></a></li>
    <% end %>
  </ul>
  
</div> 